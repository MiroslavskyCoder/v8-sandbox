{
  "name": "@bymiroslavsky/v8-sandbox",
  "version": "0.1.2",
  "description": "V8 Sandbox",
  "homepage": "http://github.com/MiroslavskyCoder/v8-sandbox",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "typings": "lib/index.d.ts",
  "scripts": {
    "configure": "node-gyp configure",
    "build": "node-gyp configure build && rollup -c rollup.config.js --bundleConfigAsCjs",
    "build-debug": "node-gyp --debug configure build && rollup -c rollup.config.js --bundleConfigAsCjs",
    "rebuild": "node-gyp configure rebuild",
    "test": "TS_NODE_IGNORE='node_modules,dist' mocha -r ts-node/register test/index.ts",
    "clean": "rm -rf dist build",
    "lint": "eslint 'lib/**/*.js'"
  },
  "author": "byMiroslavsky",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MiroslavskyCoder/v8-sandbox.git"
  },
  "files": [
    "lib",
    "source",
    "test",
    "examples",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-json": "^5.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.1",
    "@types/async": "^3.0.8",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.3",
    "@types/request": "^2.48.8",
    "@types/signal-exit": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "mocha": "^10.0.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "async": "^3.2.0",
    "axios": "^0.27.2",
    "bindings": "^1.5.0",
    "lodash": "^4.17.21",
    "nan": "^2.14.0",
    "node-gyp": "^9.0.0",
    "signal-exit": "^3.0.2"
  },
  "keywords": [
    "sandbox",
    "V8",
    "js",
    "javascript",
    "C++",
    "byMiroslavsky"
  ]
